
# 3d Lennard-Jones melt

# ---------- Initialize Simulation ---------------------
units 		metal
#boundary 	p p p
atom_style	atomic
atom_modify	map hash

# ---------- Create Atoms ---------------------
#lattice 	bcc 3.521
#region		box prism 0 4 0 4 0 4 0 0 0
#create_box	1 box
#create_atoms	1 box
#mass 		1 6.93
read_data       /Users/chinchay/Documents/Tests/testNestingSample/pymatnest/positions.dat
  orthogonal box = (0 0 0) to (10 10 10)
  1 by 1 by 1 MPI processor grid
  reading atoms ...
  2 atoms
  read_data CPU = 0.000185966 secs
mass 		      1 1.0


# ---------- Define Interatomic Potential ---------------------
pair_style mlip /Users/chinchay/Documents/Tests/testNestingSample/pymatnest/mlip.ini
pair_coeff * *
#pair_style	lj/cut 2.5
#pair_coeff	1 1 1.0 1.0 2.5
neighbor 1.5 bin


# ---------- Define Settings ---------------------
compute eng all pe/atom
compute eatoms all reduce sum c_eng

# ---------- Run ---------------------
#velocity all create 300 123456
#fix 1 all nve
thermo 100
timestep 0.001
thermo_style custom step pe ke temp press pxx pyy pzz c_eatoms

run 300
